{
  "name": "gulp-starter-kit",
  "private": true,
  "version": "1.0.0",
  "description": "Startkit for HTML/CSS/JS pages layout.",
  "author": "Ihor Sudenko",
  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/KRECER/gulp-starter_kit.git"
  },
  "scripts": {
    "lint:all": "npm run lint:style && npm run lint:js",
    "lint:style": "npx stylelint \"source/scss/**/*.scss\" --syntax=scss --color --fix",
    "lint:js": "npx eslint source/js/**/*.js --fix",
    "start": "cross-env MODE=development npx gulp",
    "build": "cross-env MODE=production npx gulp build",
    "prepare": "npx husky install",
    "deploy": "npx gulp deploy",
    "predeploy": "npm run build && npx gulp images"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "acorn": "^8.5.0",
    "autoprefixer": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "browser-sync": "^2.27.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "del": "^6.0.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "gh-pages": "^3.2.3",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-debug": "^4.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-notify": "^4.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-posthtml": "^3.0.5",
    "gulp-pretty-html": "^2.0.10",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-svgstore": "^8.0.0",
    "gulp-webp": "^4.0.1",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.4",
    "postcss": "^8.3.11",
    "postcss-inline-svg": "^5.0.0",
    "postcss-sort-media-queries": "^4.1.0",
    "posthtml-include": "^1.7.2",
    "prettier": "^2.4.1",
    "sass": "^1.43.3",
    "style-loader": "^3.3.1",
    "stylelint": "^10.0.0",
    "stylelint-config-htmlacademy": "^0.1.10",
    "stylelint-order": "^5.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "webpack": "^5.59.1",
    "webpack-stream": "^7.0.0"
  },
  "dependencies": {
    "autosize": "^5.0.1",
    "baron": "^3.0.3",
    "choices.js": "^9.0.1",
    "closest": "0.0.1",
    "swiper": "^7.1.0"
  }
}
